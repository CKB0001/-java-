     shu.artistInput();
     CD cd=new CD();
     System.out.println("请输入CD名称");
     cd.mediaNameInput();
     System.out.println("请输入价格");
     cd.mediaPriceInput();
     System.out.println("请输入出版社");
     cd.mediaPressInput();
     System.out.println("请输入作者");
     cd.artistInput();
     Tape t=new Tape();
